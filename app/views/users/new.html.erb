<%= form_for @user do |f|%>
  <%= f.label :username %><br>
  <%= f.text_field :username %><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br>
   <%= f.label :email%><br>
  <%= f.email_field :email%><br>
  <%= f.label :address%><br> 
  <%= f.text_field :address%><br>
  <%= f.label :date_birth%><br>
  <%= f.date_field :date_birth%><br>
  <%= f.label :phone_number%><br>
  <%= f.telephone_field :phone_number%><br>
  <%= f.label :credit  %> <br>
  <%= f.number_field :credit%><br>

<%end%>
