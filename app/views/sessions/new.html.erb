<h1>Log In </h1>
<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Log in" %>
<% end %>

<div>
<!-- <%= link_to "Log In", '/login' %> -->
<%= link_to "Sign Up!", '/signup' %>
</div>