<div class="record-row <%= cycle "odd", "even", name: "row-cycler" %> flexbox vertical stretch">
  <%= station.identifier %>: <%= station.name %> (<%= station.address %>)
    <% bikes_in_station = Bike.where(current_station_id: station.identifier) %>
    <b>Number of Bikes at Station: <%= bikes_in_station.count %> </b>
    <br>
    <p><b>Number of Docked Bikes:</b>
    <%= Station.find_by(identifier: station.identifier).docked_bike_count%> </p>

</div>